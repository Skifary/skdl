{
  "manifest_version": 2,

  "name": "Open In SKDL",
  "description": "Open current download in SKDL",
  "version": "0.0.1",

  "background": { 
    "scripts": [ "background.js" ] 
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open In SKDL"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
